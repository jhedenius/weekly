<h1>Hello, world!</h1>
<ul id="tasks"></ul>

<script type="text/javascript" src="jquery.js"></script>
<script type="text/javascript" src="modules.js"></script>
<script type="text/javascript">
    var db = require('db').current();

    db.getView('weekly', 'tasks', function (err, data) {
        alert('getView');
        if (err) {
            // an error occurred
            return alert(err);
        }
        for (var i = 0; i < data.rows.length; i++) {
            $('<li onClick='alert('dsds')'/>').text(data.rows[i].key).appendTo('#tasks');
        }
    });
</script>
