<script src="/_utils/script/json2.js"></script>
<script src="/_utils/script/sha1.js"></script>
<script src="/_utils/script/jquery.js?1.4.2"></script>
<script src="/_utils/script/jquery.couch.js?0.11.0"></script>
<script src="/_utils/script/jquery.dialog.js?0.11.0"></script>
<script type="text/javascript" src="modules.js"></script>
<script type="text/javascript">
    var db = require('db').current();

    db.getView('weekly', 'tasks', function (err, data) {
        alert('getView');
        if (err) {
            // an error occurred
            return alert(err);
        }
        for (var i = 0; i < data.rows.length; i++) {
            $('<li onclick=\"alert(\'dsds\')\"/>').text(data.rows[i].key).appendTo('#tasks');
        }
    });
    
    $.couch.info({
	  success: function(data) {
	      alert(data);
	  }
	});
	
</script>
